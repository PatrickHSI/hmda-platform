#!groovy
pipeline {
  agent any
  environment{
  env.NODE_HOME="${tool 'node'}"
  env.PATH="${env.NODE_HOME}/bin:${env.PATH}"
  sh 'npm -version'
  }
    stages {
        stage('Build') {
            steps {
             echo 'Building..'
             sh 'npm -version'

            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}