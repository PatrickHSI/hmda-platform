pipeline {
  agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
             nodejs(nodeJSInstallationName: 'node') {
               sh '/var/lib/jenkins/tools/jenkins.plugins.nodejs.tools.NodeJSInstallation/node/npm install -version'
             }
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}