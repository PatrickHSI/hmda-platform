#!groovy
pipeline {
  agent any
  environment{
  NODE_HOME="${tool 'node'}"
  PATH="${NODE_HOME}/bin:${PATH}"
  }
    stages {
        stage('Build') {
            steps {
             echo 'Building..'
             sh 'npm -version'

            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}