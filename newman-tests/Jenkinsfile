pipeline {
  agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
             nodejs(nodeJSInstallationName: 'node') {
               sh 'npm -version'
             }
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}