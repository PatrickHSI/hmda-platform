#!groovy
pipeline {
  agent any
  environment{

  }
    stages {
        stage('Build') {
            steps {
             echo 'Building..'
             sh '''
              NODE_HOME="${tool 'node'}"
               PATH="${NODE_HOME}/bin:${PATH}"
             npm -version'

            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}